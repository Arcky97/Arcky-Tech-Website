{
  "message": {
    "all": {
      "name": "Message Events",
      "description": "Logs all activities related to messages within the server, including edits, deletions, and bulk deletions. This provides a comprehensive record of message changes and removals for transparancy and tracking."
    },
    "edits": {
      "name": "Message Edits",
      "description": "Tracks every instance of a message being edited in the server. This includes capturing the original message, the edits made, and the user who performed the edit, ensuring a history of all message alternations."
    },
    "deletes": {
      "name": "Message Deletes",
      "description": "Logs when any message is deleted in the server. This event captures the identity of the author of the message and provides transparancy into message removal activities."
    },
    "bulks": {
      "name": "Message Bulk Deletes",
      "description": "Tracks bulk deletions of messages, where multiple message are removed at once. This event records the user who initiated the bulk delete and the number of messages affected, providing a detailed log for mass message deletions."
    }
  },
